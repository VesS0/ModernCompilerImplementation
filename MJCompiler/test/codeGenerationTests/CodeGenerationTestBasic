program doWhileeee
{
	void breakContinueTest() int i = 0; int k = 3; int t = 3; int p = 2;
	{
		do
		{
			print(i++);
			if (i>=k)
				break;
				do
				{
					do
					{
						if (p>2)
							break;
						print('D');
						p++;
					} while(true);
					p = 1;
					if (t>=k)
						break;
					t = k;
					print('C');
				} while(true);
			print('B');
			t = 0;
		} while (true);
		print('A');
	}
	
	void main() 
	{
		breakContinueTest();
	}
}// 0DB 2DDCDDB 2A