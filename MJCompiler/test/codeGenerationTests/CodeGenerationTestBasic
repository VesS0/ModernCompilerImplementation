program VesicTest
bool vlada = true;
const bool marko = true;
{
	void wrongFunction() int k;
	{
	}
	
	void main() bool testVlada; const int k = 4; const bool testMarko = true; bool mirko = true;
	{
		testVlada = mirko;
		testVlada = true;
		testVlada = vlada;
		print(k);
		mirko = vlada;
		mirko = false;
		mirko = testMarko;
		mirko = false;
		mirko = marko;
	}
}