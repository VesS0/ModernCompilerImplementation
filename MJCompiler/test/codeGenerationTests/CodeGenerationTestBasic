program VesicTest
bool vlada = true, vesic = true;
bool arrGlo[];
int globalIntArr[]; bool globalBoolArr[]; char globalCharArr[];
int x = 2, y;
bool marko = false;
{

	void boolTestFunctionLocalGlobalConst()  bool testVlada; const int k = 4; const bool testMarko = true; bool mirko = true;
	{
		testVlada = mirko;
		testVlada = true;
		testVlada = vlada;
		print(k);
		mirko = vlada;
		mirko = false;
		mirko = testMarko;
		mirko = false;
		mirko = marko;
	}
	
	void printChars() char cha = 'a', chb='b', chc='c';
	{
		cha = 'a';
		print('c'); // c
		print(cha); // a 
		print(chb); // b
		print(chc); // c
	}
	
	int incrementDecrementTestSuccessful()
	int dec, inc = 3; int kdec=5, kinc;
	{
		dec = 0;
		kinc = dec;
		dec--; inc++;
		inc++; inc--;
		
		dec = dec-- + inc++ + inc++; // expected 8
		inc = dec-- - inc++; // expected 2
		return inc + dec; // expected 9
	}
	
	int printTest() int c,k; 
	bool vlada = true;
	{
		x = 30 + 3 + x;
		c = 1 + 2*3 + (1+1)*(1+1)+2;
		vlada = false;
		vlada = true;
		
		print(x); // 35
		print(8%3); //2
		printChars();

		print(c); //13
			
		return x-c*2; // 9
	}
	
	int functionCallWithParams(int one,int k, int global, char c,bool tru)
	{
		print(one); // 1
		print(k); // 2
		print(global); //2 
		print(c); // c
		return k + one % 2;
	}
	
	void parameterArrayTesting(int intArr[], char a, bool t, char d, char charAr[], int tes)
	char localArr[];
	{
		print(intArr[9]); // 5
		print(a); // a
		print(d); //  d
		print(charAr[4]); // a
		
		localArr = new char[tes+10]; // t + 10 not working...
		localArr[tes-3] = 'V';
		localArr[tes-2] = 'E';
		localArr[tes-1] = 'S';
		localArr[tes] =   'I';
		localArr[tes+1] = 'C';
		// print(t++);
		print(localArr[tes-3]); // V
		print(localArr[tes-2]); // E
		print(localArr[tes-1]); // S
		print(localArr[tes]); // I
		print(localArr[tes+1]); // C
	}
	
	void TestingArrays(int t) int arr[],a; bool arBool[]; char arChar[]; char testing;
	{
		arr = new int[3];
		a = 5;
		arr[2] = 1;
		
		
		arr[1] = 3;
		arr[0] = a;
		
		arBool = new bool[3];
		arBool[0] = true; arBool[1] = false;
		
		arChar = new char[1];
		
		print(arr[0]); // 5
		print(arr[2]); // 1
		arChar[0] = 'd';
		
		globalIntArr = new int[10];
		globalCharArr = new char[5];
		globalBoolArr = new bool[2];
		
		globalIntArr[9] = 5;
		globalCharArr[4] = 'a';
		globalBoolArr[0] = true;
		testing = globalCharArr[4];
		
		parameterArrayTesting(globalIntArr, globalCharArr[4], globalBoolArr[0], arChar[0], globalCharArr, t);
	}
	
	void TestingIncerementDecrementOnArrays()
	int x,y,z;
	int xAr[], yAr[], zAr[];
	{
		read(x); // 3
		read(y); // 1
		read(z); // 2
		
		xAr= new int[10]; yAr= new int[10]; zAr= new int[10];
		
		xAr[0] = x; //3
		yAr[1] = y + x;
		zAr[2] = xAr[0] + yAr[1];
		
		print(xAr[0]); // 3
		print(yAr[1]); // 4
		print(zAr[2]); // 7
		
		xAr[0]--; //2
		yAr[1] = yAr[1]-- - zAr[2]++;
		print(xAr[0]); // 2
		print(yAr[1]); //-3
		print(zAr[2]); //8
		
		yAr[3] = 1;
		
		x = xAr[0]; // 2
		y = xAr[yAr[3]-1]; //2
		print(y); //2
		zAr[xAr[yAr[3]-1]]--; // 7
		print(zAr[xAr[yAr[3]-1]]--); // 7
	}
	
	void TestMulAndAddops()int k;
	{
		k = -2;
		print(k); //-2
		k = -7*2/2+8-2*4+(1+1)/2-4*3*2+2*(3-2)/2;
		print(k); //-29
		k = -(3/(2-5))*8/2;
		print(k); // 4
		k = -1+3 -3 - 5*2; // He takes 3 - 10 = -7. After this he sees -3 and does 3 - -7 and gets 10...
		print(k); // -11
		k = -2*3;
		print(k); //-6
		k = -3-4*5-2-3*2+40-(3*3-2*3)-(-(3));
		print(k); //9
	}
	
		
	void testIfElse() bool vlada= true;	int x = 3;
	int y = 2;
	{
		if (vlada)
			print(x);
		else
			print(y);
		
		vlada = false;
		
		if (vlada)
		{
			x = 10;
		}
		
		if (vlada)
		{
			print(-1);
			print(x - 1);
		}
		else
		{
			print(y - 1);
		}
	}
	
	void moreSimpleIfElseConditions(bool input) bool vlada= true;
	{
		if (vlada)
		{
			print('v');
			if (vlada)
			{
				print('l');
				if (vlada)
				{
					vlada = false;
					print('a');
				}
				print('d');
				if (vlada)
				{
					vlada = false;
				}
				else
				{
					print('a');
					if (vlada)
					{
						vlada = false;
					}
					else
					{
						print(' ');
						vlada = true;
					}
					print('v');
				}
			print('e');
			}
			else
			{
				vesic = true;
			}
			print('s');
		}
		else
		{
			if (vesic)
			{
				vlada = true;
			}
		}
		vesic = true;
		print('i');
		if (vesic)
			if (vesic)
				if (vesic)
					if (false) print('K');
					else print('c');
		print('d');
		if (true)
			print('a');
			
		if (input) print('N');
		else print('Y');
	}
	
	
	int sum(int sabiraci[])
	{
		return sabiraci[0]+ sabiraci[1];
	}
	
	int[] sumArr(int sabiraci[])
	{
		sabiraci[0] = sabiraci[0] + sabiraci[1];
		return sabiraci;
	}
	
	void readIntoArrayTestWithRecursion() int niz[], niz2[]; bool ijedva;
	{
		if (false) print(-6);
		else print(0);
		ijedva = false;
		if (ijedva)
			if (false) print (1);
			else print (-1);
		
		niz = new int[3];
		niz[0] = 1;
		niz[1] = 2;
		niz[2] = sum(niz) + 1;
		print(niz[2]);
		//if (niz[2] == 4) print(3);
		//else print(-3);
		
		read(niz[0]);
		read(niz[1]);
		read(niz[2]);
		
		niz2 = sumArr(sumArr(sumArr(niz)));
		print(niz2[0]);
	}
	
	void complexConditionsTest()bool vlada= true, vesic = false;
	{
		if (3>5)
		{
			print(-1);
		} else
		{
			print(1);
		}
		
		if (3>5 || 3>2 && 3>4 || 3>2 && vesic || 8>5 && 2>1 && 1>-1 && vlada || 8<3)
		{
			print(1);
		} else
		{
			print(-1);
		}
		
	
		if (vlada && vesic || vesic && vesic || vlada && vlada && vlada || vesic)
		{
			print('V');
		} else
		{
			print('I');
		}
		print('.');
		if (vesic && vlada)
		{
			print('B');
		}
		else
			print('V');
			
		print('.');
	}
	
	void doWhileTestt() int i = 0; int k = 8;
	{
		do
		{
			print(i++);
		} while (false || true && false || true && true && i < 8 || false);
		print('F');
	}
	
	void doWhileComplexTest() int i = 0; int k = 8; int t = 3;
	{
		if (i==0)
		{
			do
			{
				print(i++);
				print('i');
				if (i%2 == 0)
				{
					print('M');
					t = 2;
				} else
				{
					do
					{
						t--;
						print(t);
						print('t');
					} while (t>0);
					
				}
			} while (false || true && false || true && true && i < 8 || false);
		} else
		{
			print(-1);
		}
		print('E');
		print('N');
		print('D');
	}
	
	void testComplexContinue() int i = 0; int k = 4; int t = 1; int p = 5, tt=0; // 0CB    1CCB    2CCCB    3A
	{
		do
		{
			print(i);
			i++;
			if (i>=k)
				continue;
			do
			{
				t++;
				if (t+3>p)
					continue;
					
				print('C');
			} while (t<p);
			t = tt--;
			print('B');
		} while (i<k);
		print('A');
	}
	
	void testComplexBreak() int i = 0; int k = 3; int t = 3; int p = 2;
	{
		do
		{
			print(i++);
			if (i>=k)
				break;
				do
				{
					do
					{
						if (p>2)
							break;
						print('D');
						p++;
					} while(true);
					p = 1;
					if (t>=k)
						break;
					t = k;
					print('C');
				} while(true);
			print('B');
			t = 0;
		} while (true);
		print('A');
	}
	
	 void boolArrayTests() bool arrGloLoc[]; //1
	{
		arrGlo = new bool[10];
		arrGloLoc = new bool[4];
		
		arrGlo[0] = false;
		arrGlo[1] = true;
		arrGlo[2] = false;
		arrGlo[3] = true;
		
		arrGloLoc[0] = true;
		arrGloLoc[1] = false;
		arrGloLoc[2] = true;
		arrGloLoc[3] = false;
		
		if (arrGlo[0] && arrGloLoc[0] || arrGlo[1] && arrGloLoc[1] || arrGlo[3] && arrGloLoc[2] || arrGlo[0] && arrGlo[1] && arrGlo[2])
		{
			print(1);
		}
		else
		{
			print(-1);
		}
	}
	

// Inputs: c, 3, 1, 2, 5, 1, 2, 3
// Outputs:	
// 	c
//	c    93
//	1
//	2
//    3    4    7    2   -3    8    2    7    1    2    2c5
//    5    5    1    5adaVESIC   -2  -29    4  -11   -6    9   35    2cabc   13    9   12    4   35    3    1vlada vesicdaY    0    41
//	2
//	3
//    7    1    1V.V.    0    1    2    3    4    5    6    7F    0i    2t    1t    0t    1iM    2i    1t    0t    3iM    4i    1t    0t    5iM    6i    1t    0t    7iMEND    0CB    1CCB    2CCCB    3A    0DB    1DDCDDB    2A    1

	void main() int k = 2, t; char c;
	{
		read(c);
		print(c);
		print(incrementDecrementTestSuccessful()); // expected 9
		TestingIncerementDecrementOnArrays(); // 3, 4, 7, 2, -3, 8, 2, 7,
		functionCallWithParams(1, k, x, 'c', true); // 1, 2, 2, c
		read(t); //3  or more!
		print(t);
		TestingArrays(t); // 5, 1, 5, a, d, a, V E S I C

		TestMulAndAddops(); // -2, -29, 4, -11, -6, 9
	
		y = printTest(); // 35, 2, c, a ,b ,c, 13
		print(y); // 9
		print((y + 1 + k)); // 12
		boolTestFunctionLocalGlobalConst(); //4
		print(x); // 35
		testIfElse(); // 3, 1
		vesic = false;
		moreSimpleIfElseConditions(false); // valada vesicda Y
		readIntoArrayTestWithRecursion(); // input 1,2,3 // output 0 4 7
		complexConditionsTest(); // 1, 1, V, .,  V, .
		doWhileTestt(); // 0, 1, 2, 3, 4, 5, 6, 7, F
		doWhileComplexTest(); //0i    2t    1t    0t    1iM    2i    1t    0t    3iM    4i    1t    0t    5iM    6i    1t    0t    7iMEND
		testComplexContinue(); // 0CB    1CCB    2CCCB    3A
		testComplexBreak();  // 0DB 2DDCDDB 2A
		boolArrayTests(); // 1
	}
}